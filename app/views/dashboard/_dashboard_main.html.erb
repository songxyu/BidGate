<div class="dashboard-page">
	<div class="left-list grid_layout_area">

		<div class="ui-side ui-side-withicon" id="sidebar">
			<ul class="ui-side-list">
				
				<li class="ui-side-item ">
					<%= link_to dashboard_orders_path, :class=>"menuItem", remote: true  do %>
					<a class="ui-side-item-link">订单管理<i class="fn-left  icon ui-icon-order"></i></a>
					<% end %>
				</li>
				
				<li class="ui-side-item active">
					<%= link_to dashboard_path, :class=>"menuItem", remote: true  do %>
					<a class="ui-side-item-link" href="">我的壹百万<i class="fn-left icon ui-icon-home"></i></a>
					<% end %>
				</li>
				
				<li class="ui-side-item ">
					<%= link_to dashboard_msg_path, :class=>"menuItem", remote: true do %>
					<a class="ui-side-item-link">消息管理<i class="fn-left icon ui-icon-msgs"></i></a>
					<% end %>
				</li>

			</ul>
			 
		</div>

	</div>
	
	<div class="right-area grid_layout_area">
		
		<%= render "dashboard/my_profile" %>
		
		
	</div>

</div>